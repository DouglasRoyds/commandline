openssl genrsa -out rsakey.pem 4096                Generate a private key
                                                      -aes256     Encrypt the private key
openssl rsa -noout -text -in rsakey.pem            Dump the private key
openssl rsa -pubout -in rsakey.pem                 Print the public key

openssl x509 -noout -text -in root.pem             Dump a certificate to stdout

openssl s_client -tls1_3 -connect hostname:443     Does the host support TLS 1.3?
                                                      -tls1_2
                                                      -tls1_1
                                                      -tls1
